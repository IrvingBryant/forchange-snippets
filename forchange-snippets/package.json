{
    "name": "forchange-snippets",
    "displayName": "forchange-snippets",
    "description": "Code snippets for internal forchange company",
    "version": "0.0.8",
    "icon":"icon/icon.jpg",
    "publisher": "IrvingYang",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Snippets"
    ],
    "repository": { 
        "type": "git", 
        "url":"git+https://github.com/IrvingBryant/forchange-snippets.git"
    },
    "contributes": {
        "snippets": [
            {
                "language": "vue",
                "aliases":["forvue","FORVUE"],
                "path": "./snippets/vue-snippets.json"
            },
            {
                "language": "javascript",
                "aliases":["forapis","FORAPIS"],
                "path": "./snippets/apis-snippets.json"
            },
            {
                "language": "javascript",
                "aliases":["forroute","FORROUTE"],
                "path": "./snippets/routes-snippets.json"
            }
        ]
    },
    "bugs": {
        "url": "https://github.com/IrvingBryant/forchange-snippets/issues"
      },
      "homepage": "https://github.com/IrvingBryant/forchange-snippets#readme"
}